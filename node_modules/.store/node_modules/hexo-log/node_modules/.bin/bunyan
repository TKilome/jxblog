#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/G:/BigData26/djxblog/blog/node_modules/.store/hexo-bunyan@1.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/G:/BigData26/djxblog/blog/node_modules/.store/hexo-bunyan@1.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../hexo-bunyan@1.0.0/node_modules/hexo-bunyan/bin/bunyan" "$@"
else
  exec node  "$basedir/../../../../../hexo-bunyan@1.0.0/node_modules/hexo-bunyan/bin/bunyan" "$@"
fi
